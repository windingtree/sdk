{
  "name": "@windingtree/sdk",
  "version": "0.0.0",
  "description": "The WindingTree market protocol SDK",
  "repository": "git@github.com:windingtree/sdk.git",
  "author": "Kostiantyn Smyrnov <kostysh@gmail.com>",
  "contributors": [
    "Kostiantyn Smyrnov <kostysh@gmail.com>",
    "Evgenij Novikov <galeaf11@gmail.com>"
  ],
  "bugs": "https://github.com/windingtree/sdk/issues",
  "homepage": "https://github.com/windingtree/sdk#readme",
  "keywords": [
    "windingtree",
    "market",
    "protocol",
    "libp2p",
    "sdk",
    "typescript",
    "p2p",
    "peer",
    "peer-to-peer",
    "network"
  ],
  "license": "MIT",
  "private": true,
  "publishConfig": {
    "access": "restricted"
  },
  "type": "module",
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "@vitest/coverage-v8": "^0.33.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "lerna": "^7.1.4",
    "prettier": "^3.0.0",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.4",
    "vite-plugin-dts": "^3.3.1",
    "vite-plugin-node-polyfills": "^0.9.0",
    "vitest": "^0.33.0"
  },
  "scripts": {
    "prepare": "husky install",
    "commit": "git-cz -S",
    "build": "pnpm -r --if-present run build",
    "lint": "pnpm -r --parallel --if-present run lint",
    "lint:fix": "pnpm -r --parallel --if-present run lint:fix",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "example:server": "DEBUG=* pnpm -C examples/server run start",
    "example:node": "DEBUG=* pnpm -C examples/node run start",
    "example:client": "pnpm -C examples/client run start",
    "example:manager": "pnpm -C examples/manager run start"
  }
}
